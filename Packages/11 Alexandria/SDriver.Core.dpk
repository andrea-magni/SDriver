{$DEFINE DEBUG}
{$ENDIF IMPLICITBUILDING}
{$DESCRIPTION 'SDriver Core'}
{$LIBSUFFIX '280'}
{$RUNONLY}
{$IMPLICITBUILD ON}

requires
